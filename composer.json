{
  "name": "msbios/widget",
  "description": "MSBios Widget",
  "license": "GPL",
  "keywords": [
    "php"
  ],
  "homepage": "https://github.com/msbios/msbios-widget",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Judzhin Miles",
      "email": "info@msbios.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "MSBios\\Widget\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MSBiosTest\\Widget\\": "tests/"
    }
  },
  "require": {
    "zendframework/zend-loader": "^2.5",
    "zendframework/zend-servicemanager": "^3.3",
    "zendframework/zend-modulemanager": "^2.8",
    "zendframework/zend-view": "^2.9",
    "msbios/msbios": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "zendframework/zend-test": "^3.1",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": [
      "@cs-check",
      "phpunit"
    ],
    "commit": "git add . && git commit -a -m \"Lazy Composer Commiter\" && git push"
  }
}
